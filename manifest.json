{
"update_url": "https://clients2.google.com/service/update2/crx",

  "name": "Imgur Engel Kaldir",
  "description": "Imgur'un engelini kaldirir",
  "version": "1.0",
  "manifest_version": 2,
  "background": {"scripts":["background.js"]},
  "icons": { "16": "w16.png",
           "48": "w48.png",
          "128": "w128.png" },
  "permissions": [
    "webRequest",
    "*://imgur.com*",
    "*://www.imgur.com/*",
	"*://*.imgur.com/*",
    "webRequestBlocking"
  ]
}